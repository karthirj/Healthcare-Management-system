<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthcare">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".AppointmentHistoryActivity"></activity>
        <activity android:name=".LabAppointments" />
        <activity android:name=".PharmacyAppointments" />
        <activity android:name=".DoctorAppointments" />
        <activity android:name=".Book_Appointment" />
        <activity android:name=".LabMainActivity" />
        <activity android:name=".LabSignUpActivity" />
        <activity android:name=".LabLoginActivity" />
        <activity android:name=".PharmacyMainActivity" />
        <activity android:name=".PharmacySignupActivity" />
        <activity android:name=".PharmacyLoginActivity" />
        <activity android:name=".DoctorMainActivity" />
        <activity android:name=".DoctorSignupActivity" />
        <activity android:name=".DoctorLoginActivity" />
        <activity android:name=".PatientDashboardActivity" />
        <activity android:name=".PatientSignupActivity" />
        <activity android:name=".PatientLoginActivity" />
        <activity android:name=".SelectActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>